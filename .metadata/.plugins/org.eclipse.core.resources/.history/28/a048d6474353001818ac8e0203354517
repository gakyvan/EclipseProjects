package demo.adapter;

public class Adaptee {

	private String[] data;
	private int start;
	private int end;
	
	public boolean isEmpty() {
		return end==0;
	}
	
	public boolean isFull() {
		return data.length-1==end;
	}
	
	public Adaptee() {
		data=new String[10];
		start=0;
		end=0;
	}

	public String[] getData() {
		return data;
	}

	public int getStart() {
		return start;
	}

	public int getEnd() {
		return end;
	}

}
