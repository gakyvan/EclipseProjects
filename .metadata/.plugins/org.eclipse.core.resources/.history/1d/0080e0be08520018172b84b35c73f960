package asdw1d3_1;

import java.util.ArrayList;
import java.util.Arrays;

public class Test {
	
	static ArrayList<String> a= new ArrayList<String>(Arrays.asList("5","14","10","20"));
	static ArrayList<String> b= new ArrayList<String>(Arrays.asList("9","14","10","20"));
	static  ArrayList<String> c= new ArrayList<String>(Arrays.asList("9","8","10","20"));
	static String typeA= "VERTICAL";
	static String typeB= "NONE";
	static String typeC= "HORIZONTAL";
	
	
	public static void main(String[] args){
	 AlphabetSymmetryClass mysym= new AlphabetSymmetryClass();
	 mysym.FullImage(typeA, a);
	 mysym.FullImage(typeB, b);
	 mysym.FullImage(typeC, c);
	 
	}	

}
